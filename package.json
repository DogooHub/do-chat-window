{
  "name": "do-chat-window",
  "version": "1.1.22",
  "description": "Chat window for products of Dogoo Office",
  "author": "longnvse",
  "license": "MIT",
  "repository": "https://github.com/DogooHub/do-chat-window",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "autoprefixer": "^9.8.8",
    "clean-publish": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.39",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "antd": "^5.8.3",
    "axios": "^1.4.0",
    "babel": "^6.23.0",
    "dayjs": "^1.11.9",
    "html-react-parser": "^4.2.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "open-im-sdk": "^2.3.0-beta.2",
    "prop-types": "^15.8.1",
    "rc-virtual-list": "^3.5.3",
    "react-icons": "^4.10.1",
    "sass": "^1.65.1",
    "styled-components": "^6.0.7"
  }
}
